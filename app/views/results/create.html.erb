<div class="container">

	<h1><%= "Updated Member (Barcode " + params[:barcode] + ')' %> </h1>

	<strong><%= "_id" %></strong><%=" : " + params[:dbID] %><br>
	<strong><%= "_rev" %></strong><%=" : " + params[:dbRev] %><br>
	<% params[:result].each do |elem| %>
		<strong><%= elem.at(0) %></strong><%=" : " + elem.at(1)%><br>
	<% end %>


</div>